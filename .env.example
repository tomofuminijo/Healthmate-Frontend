# Environment Configuration Template
# Copy this file to .env and fill in the actual values

# Environment (dev/stage/prod)
HEALTHMATE_ENV=dev

# AWS Configuration
VITE_AWS_REGION=us-west-2
AWS_REGION=us-west-2

# Cognito Configuration
VITE_COGNITO_USER_POOL_ID=
VITE_COGNITO_CLIENT_ID=
VITE_COGNITO_REGION=us-west-2

# API Endpoints
VITE_API_BASE_URL=http://localhost:3000

# CoachAI Configuration
VITE_COACHAI_AGENT_ARN=arn:aws:bedrock-agentcore:us-west-2:123456789012:runtime/healthmate_coach_ai

# Logging Configuration
VITE_LOG_LEVEL=DEBUG

# Deployment Configuration (Optional - will be auto-populated from CloudFormation)
# HEALTHMATE_FRONTEND_DEV_BUCKET_NAME=
# HEALTHMATE_FRONTEND_DEV_DISTRIBUTION_ID=
# HEALTHMATE_FRONTEND_DEV_DISTRIBUTION_DOMAIN=

# AWS Credentials (Optional - use AWS CLI profile or IAM roles instead)
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_PROFILE=default